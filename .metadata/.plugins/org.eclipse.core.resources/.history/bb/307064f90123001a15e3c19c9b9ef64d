package com.java8;

import java.util.Optional;

public class OptionalClass {
	
	public static void main(String[] args) {
		
		
		Optional<String> opt=Optional.empty();
		
		//System.out.println(opt);
		
		String book="";
		
		if(book!=null)
		{
			System.out.println(book.toUpperCase());
		}
		
		
	}

}
