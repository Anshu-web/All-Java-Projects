public class Test11 {
	
	public static void main(String[] args) {
		
   D d=new D();
   C c=new C();
   d=c;
	 
	}
}

 

 class A
{
	public void printName()
	{
		System.out.println("Value-A");
	}
}
 class D extends A
{
	 public void printName()
		{
			System.out.println("Value-D");
		}
}
 class C extends A
{
	 public void printName()
		{
			System.out.println("Value-C");
		}
}


